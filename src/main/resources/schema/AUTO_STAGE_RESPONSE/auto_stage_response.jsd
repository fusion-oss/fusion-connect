                "description": "Unique identifier of the input (generally the LPN of a tote).",
                "default": "",
                "examples": [
                  "C00001"
                ],
                "maxLength": 30
              },
              "inputType": {
                "$id": "#/properties/containers/items/anyOf/0/properties/inputType",
                "type": "string",
                "title": "Input Type",
                "description": "Type of input (currently only CONTAINER_ID is supported).",
                "default": "",
                "examples": [
                  "CONTAINER_ID"
                ],
                "pattern": "^CONTAINER_ID$"
              },
              "orderId": {
                "$id": "#/properties/containers/items/anyOf/0/properties/orderId",
                "type": "string",
                "title": "Order ID",
                "description": "Order ID of the container.",
                "default": "",
                "examples": [
                  "12345678"
                ],
                "maxLength": 8
              },
              "location": {
                "$id": "#/properties/containers/items/anyOf/0/properties/location",
                "type": "string",
                "title": "Location",
                "description": "Location where the tote was scanned.",
                "default": "",
                "examples": [
                  "MFC"
                ],
                "pattern": "^HOSPITAL|MFC|WORKSTATION_[0-9]{2}$"
