                ],
                "pattern": "^CONTAINER_ID$"
              },
              "orderId": {
                "$id": "#/properties/containers/items/anyOf/1/properties/orderId",
                "type": "string",
                "title": "Order ID",
                "description": "Order ID of the container.",
                "default": "",
                "examples": [
                  "12345678"
                ],
                "maxLength": 8
              },
              "location": {
                "$id": "#/properties/containers/items/anyOf/1/properties/location",
                "type": "string",
                "title": "Location",
                "description": "Location where the tote was scanned.",
                "default": "",
                "examples": [
                  "MFC"
                ],
                "pattern": "^HOSPITAL|MFC|WORKSTATION_[0-9]{2}$"
              },
              "autoStageAllowed": {
                "$id": "#/properties/containers/items/anyOf/1/properties/autoStageAllowed",
                "type": "boolean",
                "title": "Auto-Stage Allowed",
