{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "Auto-Stage Response",
  "description": "The “Auto-Stage Response” interface provides details back the MFC about whether a store-picked tote can be staged or not.",
  "default": {},
  "examples": [
    {
      "containers": [
        {
          "inputId": "C00001",
          "inputType": "CONTAINER_ID",
          "orderId": "12345678",
          "location": "MFC",
          "autoStageAllowed": true,
          "containerId": "C00001"
        },
        {
          "inputId": "C00003",
          "inputType": "CONTAINER_ID",
          "orderId": "12345678",
          "location": "MFC",
          "autoStageAllowed": false,
          "containerId": "C00003",
          "responseCode": "62",
          "responseText": "container number is not in right status (DISPENSED)"
        }
      ]
    }
  ],
  "required": [
    "containers"
  ],
  "properties": {
    "containers": {
      "$id": "#/properties/containers",
      "type": "array",
      "title": "Containers",
      "description": "List of containers for inbound into the MFC for staging.",
